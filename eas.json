{
  "cli": {
    "version": ">= 15.0.14",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "resourceClass": "default"
      },
      "cache": {
        "disabled": true
      }
    },
    "production": {
      "ios": {
        "distribution": "store",
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "resourceClass": "default"
      },
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production",
        "NPM_CONFIG_PACKAGE_LOCK_ONLY": "false",
        "EXPO_NO_CAPABILITY_SYNC": "1"
      },
      "cache": {
        "disabled": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
