{
  "expo": {
    "name": "KuryeX",
    "slug": "kuryex",
    "platforms": [
      "ios",
      "android"
    ],
    "web": {
      "enabled": false,
      "favicon": "./assets/favicon.png"
    },
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "kuryex",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*",
      "assets/sounds/**/*"
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#3B82F6",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "KuryeX Bildirimi"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kuryex.app",
      "buildNumber": "83",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "1.0"
            },
            "192.168.1.105": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "1.0"
            },
            "kuryex.enucuzal.com": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionRequiresForwardSecrecy": false,
              "NSExceptionMinimumTLSVersion": "1.0",
              "NSIncludesSubdomains": true
            },
            "enucuzal.com": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionRequiresForwardSecrecy": false,
              "NSExceptionMinimumTLSVersion": "1.0",
              "NSIncludesSubdomains": true
            },
            "ec2-16-171-131-126.eu-north-1.compute.amazonaws.com": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionRequiresForwardSecrecy": false,
              "NSExceptionMinimumTLSVersion": "1.0",
              "NSIncludesSubdomains": true
            }
          }
        },
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Uygulama, profil fotoğrafı eklemeniz veya makbuz tarama gibi özellikler için kameranıza erişmek istiyor.",
        "NSPhotoLibraryUsageDescription": "Uygulama, sipariş görselleri ve profil fotoğrafları için galerinizdeki fotoğraflara erişmek istiyor.",
        "NSLocationWhenInUseUsageDescription": "Konumunuz, siparişleri harita üzerinde takip etmek ve size en yakın kuryeleri bulmak için kullanılır.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Bu uygulama, kurye olarak çalıştığınızda müşterilere gerçek zamanlı teslimat takibi sağlamak, size en yakın siparişleri önermek ve ETA hesaplamaları yapmak için konumunuzu kullanır. Bu özellik müşteri memnuniyetini artırır ve teslimat sürecini optimize eder.",
        "NSLocationAlwaysUsageDescription": "Aktif teslimat sırasında müşteriye gerçek zamanlı kurye takibi sağlamak, otomatik teslim bildirimi göndermek ve güvenlik amaçlı acil durum tespit sistemi için arka plan konum erişimi gereklidir. Bu özellik sadece aktif sipariş sırasında çalışır.",
        "UIBackgroundModes": [
          "remote-notification",
          "processing",
          "fetch",
          "location"
        ],
        "BGTaskSchedulerPermittedIdentifiers": [
          "com.expo.modules.backgroundtask.processing",
          "com.expo.modules.backgroundtask.processing"
        ],
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationPortrait"
        ]
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDp_lLu5U3bFyMt1ggWM1p2OpblYKMQyQc"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION"
      ],
      "versionCode": 83,
      "package": "com.kuryex.app"
    },
    "plugins": [
      "expo-dev-client",
      "expo-router",
      "expo-splash-screen",
      "expo-secure-store",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#3B82F6",
          "sounds": [
            "./assets/sounds/ring_bell2.wav"
          ],
          "iosDisplayInForeground": true,
          "ios": {
            "icon": "./assets/icon.png",
            "color": "#3B82F6",
            "sounds": [
              "ring_bell2.wav"
            ],
            "requestPermissionsOnStartup": true
          },
          "android": {
            "icon": "./assets/icon.png",
            "color": "#3B82F6",
            "sounds": [
              "./assets/sounds/ring_bell2.wav"
            ],
            "channelId": "kuryex-notifications",
            "channelName": "KuryeX Bildirimler",
            "channelDescription": "KuryeX uygulaması bildirimlerini alın",
            "channelImportance": 4,
            "requestPermissionsOnStartup": true
          }
        }
      ],
      "expo-dev-client",
      [
        "expo-image-picker",
        {
          "photosPermission": "Uygulama, sipariş görselleri ve profil fotoğrafları için galerinizdeki fotoğraflara erişmek istiyor.",
          "cameraPermission": "Uygulama, profil fotoğrafı eklemeniz veya makbuz tarama gibi özellikler için kameranıza erişmek istiyor."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useModularHeaders": true
          },
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "buildToolsVersion": "35.0.0",
            "packagingOptions": {
              "pickFirst": ["**/libc++_shared.so", "**/libjsc.so"]
            }
          }
        }
      ],
      "expo-audio",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Kurye olarak sipariş teslim ederken, uygulama arka planda çalışırken bile konumunuzu paylaşmamız gerekiyor.",
          "locationAlwaysPermission": "Sipariş teslimatı sırasında sürekli konum takibi gerekiyor.",
          "locationWhenInUsePermission": "Konumunuz, siparişleri harita üzerinde takip etmek için kullanılır.",
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      "expo-task-manager"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "API_HOST": "kuryex.enucuzal.com",
      "API_PORT": "80",
      "REMOTE_API_HOST": "https://kuryex.enucuzal.com",
      "USE_REMOTE": true,
      "eas": {
        "projectId": "2b9b6713-2a3b-4fc7-af89-b8b17f3a7e91"
      }
    },
    "jsEngine": "jsc",
    "runtimeVersion": "1.1.0",
    "updates": {
      "url": "https://u.expo.dev/2b9b6713-2a3b-4fc7-af89-b8b17f3a7e91"
    }
  }
}
